   "source": [
    "s = input('Enter the string: ')\n",
    "s1 = s.lower()\n",
    "print(s1)\n",
    "l = len(s1)\n",
    "check = True\n",
    "for i in range(l//2):\n",
    "    if s[i]!=s[-(i+1)]:\n",
    "        check = False\n",
    "        break\n",
    "\n",
    "if check:\n",
    "    print('Palindrome')\n",
    "else:\n",
    "    print('Not Palindrome')"
   ]
  }
